{
  "name": "react-finland-site",
  "version": "0.0.0",
  "description": "React Finland site",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node ./antwar.bootstrap.js develop",
    "build": "node ./antwar.bootstrap.js build && npm run sitemap && npm run copy:all",
    "build:netlify": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ext js --ext jsx --ignore-path .gitignore --cache",
    "test": "npm run lint",
    "sitemap": "cd build && sitemap-static --prefix=https://react-finland.fi/ > sitemap.xml",
    "prepush": "npm test",
    "copy:all": "npm run copy:extra",
    "copy:extra": "cp -rfv assets/extra/* build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ReactFinland/site.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ReactFinland/site/issues"
  },
  "homepage": "https://github.com/ReactFinland/site#readme",
  "dependencies": {
    "@react-finland/content-2018": "^9.0.3",
    "@survivejs/components": "^0.2.0",
    "antwar": "^0.21.1",
    "moment": "^2.20.1",
    "normalize.css": "^7.0.0",
    "object.values": "^1.0.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-country-flag": "^0.1.12",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.1.4",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.0.9",
    "preact": "^8.2.7",
    "preact-compat": "^3.17.0",
    "prettier": "^1.9.2",
    "raw-loader": "^0.5.1",
    "react-router": "^4.2.0",
    "sitemap-static": "^0.4.2",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
